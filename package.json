{
  "name": "Vertere3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "android:dev": "cross-env ENVFILE=.env.development react-native run-android",
    "android:staging": "cross-env ENVFILE=.env.staging react-native run-android",
    "android:prod": "cross-env ENVFILE=.env.production react-native run-android",
    "android-linux-build:dev": "cd android && gradlew assembleDebug -PenvFile=.env.development",
    "android-linux-build:staging": "cd android && gradlew assembleRelease -PenvFile=.env.staging",
    "android-linux-build:prod": "cd android && gradlew assembleRelease -PenvFile=.env.production",
    "android-win-build:dev": "cross-env ENVFILE=.env.development cd android && gradlew.bat assembleDebug",
    "android-winbuild:staging": "cross-env ENVFILE=.env.staging cd android && gradlew.bat assembleRelease",
    "android-winbuild:prod": "cross-env ENVFILE=.env.production cd android && gradlew.bat assembleRelease",
    "ios:dev": "ENVFILE=.env.development react-native run-ios",
    "ios:staging": "ENVFILE=.env.staging react-native run-ios",
    "ios:prod": "ENVFILE=.env.production react-native run-ios",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/stack": "^7.1.1",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-config": "^1.5.3",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-paper": "^5.12.5",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-vector-icons": "^10.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
